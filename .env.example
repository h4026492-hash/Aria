# Example environment variables for Aria
# Copy to `.env` and fill in values

# Groq API
GROQ_API_KEY=

# D-ID API (cloud provider)
D_ID_API_KEY=

# SadTalker server auth
SADTALKER_API_KEY=
SADTALKER_ROOT=
SADTALKER_CACHE_DIR=server/results
SADTALKER_CACHE_TTL=604800 # 7 days in seconds

# Other options
NODE_ENV=development
PORT=5173